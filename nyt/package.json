  "devDependencies": {
    "concurrently": "^3.5.0",
    "nodemon": "^1.18.2"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "mongoose": "^4.11.7",
    "morgan": "^1.9.0",
